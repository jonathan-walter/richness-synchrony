pvals<-rbind(drt_coral_results$synch.sig, hay_results$synch.sig, jrg_results$synch.sig, jrn_basn_results$synch.sig,
             jrn_ibpe_results$synch.sig, jrn_summ_results$synch.sig, knz_t_lowland_results$synch.sig,
             knz_f_upland_results$synch.sig,lok_coral_results$synch.sig, mau_coral_results$synch.sig,
             mcr_coral_algae_backreef_results$synch.sig, mcr_coral_algae_fringing_results$synch.sig, 
             mcr_coral_algae_outer10_results$synch.sig, mdk_coral_results$synch.sig,
             sbc_results$synch.sig, sev_b_results$synch.sig,sev_c_results$synch.sig,sev_g_results$synch.sig,
             upk_coral_results$synch.sig, usvi_coral_results$synch.sig)

pvals<-data.frame(community=c("drt","hay","jrg","jrn_basn","jrn_ibpe","jrn_summ","knz_lowland","knz_upland",
                          "lok","mau","mcr_backreef","mcr_fringing","mdk","mcr_outer10","sbc","sev_b",
                          "sev_c","sev_g","upk","usvi"), pvals)

write.csv(pvals,"./GeogSynch/Scripts/final_data/synchrony_sig_test_results.csv", row.names=F)
